<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircularMenu">
        <attr name="itemCount" format="integer" />

        <attr name="innerRadius" format="dimension"/>
        <attr name="outerRadius" format="dimension"/>

        <attr name="strokeWidth" format="dimension"/>

        <attr name="textSize" format="dimension"/>
        <attr name="centerTextSize" format="dimension"/>
        <attr name="centerText" format="string"/>

        <attr name="strokeColor" format="color"/>
        <attr name="innerCircleColor" format="color"/>
        <attr name="centerTextColor" format="color"/>
        <attr name="selectedItemColor" format="color"/>
        <attr name="defaultItemsColor" format="color"/>
    </declare-styleable>

</resources>